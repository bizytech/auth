{
    "name": "nisimpo/auth",
    "description": "Nisimpo UI utilities and presets.",
    "keywords": ["nisimpo", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shabani Nzogo",
            "email": "nzogo05@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9.21|^10.0|^11.0",
        "illuminate/filesystem": "^9.21|^10.0|^11.0",
        "illuminate/support": "^9.21|^10.0|^11.0",
        "illuminate/validation": "^9.21|^10.0|^11.0",
        "spatie/laravel-permission": "^6.4",
        "symfony/console": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.35|^8.15|^9.0",
        "phpunit/phpunit": "^9.3|^10.4|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Nisimpo\\Auth\\": "src/",
            "Nisimpo\\Auth\\Tests\\": "tests/",
            "Illuminate\\Foundation\\Auth\\": "auth-backend/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.01-dev"
        },
        "laravel": {
            "providers": [
                "Nisimpo\\Auth\\UiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
